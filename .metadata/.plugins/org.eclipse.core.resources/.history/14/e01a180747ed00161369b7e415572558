import java.util.Scanner;
import java.util.ArrayList;

public class Mascota {
		
	Scanner sc=new Scanner(System.in);
	public static void main(String[] args) {
		
		
		Mascota datosMascota = new Mascota();
		
		datosMascota.nombreMascota();
		datosMascota.edadMascota();
		datosMascota.razaMascota();

	}
	
	public void nombreMascota(){
		
		ArrayList<String>nombreMascota=new ArrayList<String>();
		nombreMascota.add("Firualis");
		nombreMascota.add("Rambo");
		
		System.out.println(nombreMascota);
		
	}
	public void edadMascota(){
		
		ArrayList<String>edadMascota=new ArrayList<String>();
		edadMascota.add("4");
		edadMascota.add("6");
		System.out.println(edadMascota);
		
	}
	
	public void razaMascota(){
		
		ArrayList<String>razaMascota=new ArrayList<String>();
		razaMascota.add("Dalmata");
		razaMascota.add("Pitbull");
		System.out.println(razaMascota);
		
	}
	
}
