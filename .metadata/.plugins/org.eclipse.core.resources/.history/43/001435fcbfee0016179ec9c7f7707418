import java.util.Scanner;

public class MenorAMayor {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Introduce numero 1:");
		int n1 = sc.nextInt();
		System.out.println("Introduce numero 2:");
		int n2 = sc.nextInt();
		System.out.println("Introduce numero 3:");
		int n3 = sc.nextInt();

		if (n1 > n2 && n1 > n3) {
			int mayor = n1; 
		} else if (n2 > n1 && n2 > n3) {
			int mayor = n2;
		} else if (n3 > n1 && n3 > n2) {
			int mayor = n3;
		} else if (n1 < n2 && n1 < n3) {
			int menor = n1;
		} else if (n2 < n1 && n2 < n3) {
			int menor = n2;
		} else if (n3 > n1 && n3 > n2) {
			int menor = n3;
		}

	}

}
